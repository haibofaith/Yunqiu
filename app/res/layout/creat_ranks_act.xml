<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@color/color_home_bg"
    >

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:scrollbars="none">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingBottom="50dp"
            android:orientation="vertical">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                >

                <ImageView
                    android:id="@+id/message_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:padding="15dp"
                    android:src="@drawable/back_img"
                    android:textColor="@color/white"
                    />


                <TextView
                    android:id="@+id/bg_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:padding="15dp"
                    android:text="背景图"
                    android:textColor="@color/color_green"
                    android:textSize="14sp"
                    />

                <TextView
                    android:id="@+id/creat_ranks_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:padding="15dp"
                    android:text="创建球队"
                    android:textColor="@color/color_63"
                    android:textSize="14sp"
                    />


            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="160dp"
                android:id="@+id/cloud_home_bg"
                android:background="@drawable/cloud_home_bg"
                >

                <com.kball.function.other.CircleImageView
                    android:id="@+id/rank_img"
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="30dp"
                    android:background="@drawable/create_duihui"
                    />


            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:padding="15dp"
                >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:drawablePadding="5dp"
                    android:drawableRight="@drawable/red_bg"
                    android:text="球队名"
                    android:textColor="@color/color_63"
                    android:textSize="14sp"
                    />

                <EditText
                    android:id="@+id/rank_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:background="#00000000"
                    android:hint="16字以内，点击输入"
                    android:maxLength="16"
                    android:gravity="right"
                    android:textColor="@color/color_63"
                    android:textSize="14sp"
                    />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rank_type_rel"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:background="@color/white"
                android:padding="15dp"
                >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:drawablePadding="5dp"
                    android:drawableRight="@drawable/red_bg"
                    android:text="球队类型"
                    android:textColor="@color/color_63"
                    android:textSize="14sp"
                    />

                <TextView
                    android:id="@+id/type_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="15dp"
                    android:background="#00000000"
                    android:textColor="@color/color_69"
                    android:textSize="14sp"
                    />

                <ImageView
                    android:layout_width="8dp"
                    android:layout_height="12dp"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:background="@drawable/return_to_img"
                    />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/add_tv"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:background="@color/white"
                android:padding="15dp"
                >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:drawablePadding="5dp"
                    android:drawableRight="@drawable/red_bg"
                    android:text="地区"
                    android:textColor="@color/color_63"
                    android:textSize="14sp"
                    />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="15dp"
                    android:background="#00000000"
                    android:id="@+id/diqu_tv"
                    android:textColor="@color/color_69"
                    android:textSize="14sp"
                    />

                <ImageView
                    android:layout_width="8dp"
                    android:layout_height="12dp"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:background="@drawable/return_to_img"
                    />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.2dp"
                android:layout_marginLeft="15dp"
                android:background="#e4e4e4"
                />

            <RelativeLayout
                android:id="@+id/place_rel"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:padding="15dp"
                >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:drawablePadding="5dp"
                    android:drawableRight="@drawable/red_bg"
                    android:text="主场"
                    android:textColor="@color/color_63"
                    android:textSize="14sp"
                    />

                <TextView
                    android:id="@+id/place_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="15dp"
                    android:background="#00000000"
                    android:textColor="@color/color_69"
                    android:textSize="14sp"
                    />

                <ImageView
                    android:layout_width="8dp"
                    android:layout_height="12dp"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:background="@drawable/return_to_img"
                    />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/time_rel"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:background="@color/white"
                android:padding="15dp"
                >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="成立时间"
                    android:textColor="@color/color_63"
                    android:textSize="14sp"
                    />

                <TextView
                    android:id="@+id/time_tv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="15dp"
                    android:background="#00000000"
                    android:textColor="@color/color_69"
                    android:textSize="14sp"
                    />

                <ImageView
                    android:layout_width="8dp"
                    android:layout_height="12dp"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:background="@drawable/return_to_img"
                    />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/tag_rel"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:background="@color/white"
                android:paddingBottom="10dp"
                android:paddingLeft="15dp"
                android:paddingRight="15dp"
                android:paddingTop="10dp"
                >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="球队标签"
                    android:textColor="@color/color_63"
                    android:textSize="14sp"
                    />



                <ImageView
                    android:layout_width="8dp"
                    android:layout_height="12dp"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:background="@drawable/return_to_img"
                    />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.2dp"
                android:background="#e4e4e4"
                />

            <HorizontalScrollView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:scrollbars="none"
                >

                <LinearLayout
                    android:id="@+id/tag_lin"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:visibility="gone"
                    android:orientation="horizontal"
                    android:padding="15dp"
                    >
                </LinearLayout>
            </HorizontalScrollView>


            <TextView
                android:id="@+id/creat_btn"
                android:layout_width="120dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="30dp"
                android:background="@drawable/yq_btn_green"
                android:gravity="center"
                android:paddingBottom="10dp"
                android:paddingTop="10dp"
                android:text="创建"
                android:textColor="@color/white"
                android:textSize="16sp"
                />
        </LinearLayout>
    </ScrollView>


    <View
        android:id="@+id/zz_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#60000000"
        android:visibility="gone"
        />

    <LinearLayout
        android:id="@+id/rank_type_show_rel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:background="@color/white"
        android:orientation="vertical"
        android:visibility="gone"
        >

        <TextView
            android:id="@+id/type_1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:padding="15dp"
            android:text="业余爱好"
            android:textColor="@color/color_63"
            android:textSize="14sp">
        </TextView>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.2dp"
            android:background="#e4e4e4"
            />

        <TextView
            android:id="@+id/type_2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:padding="15dp"
            android:text="校园球队"
            android:textColor="@color/color_63"
            android:textSize="14sp">
        </TextView>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.2dp"
            android:background="#e4e4e4"
            />

        <TextView
            android:id="@+id/type_3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:padding="15dp"
            android:text="公司球队"
            android:textColor="@color/color_63"
            android:textSize="14sp">
        </TextView>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.2dp"
            android:background="#e4e4e4"
            />

        <TextView
            android:id="@+id/type_4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:padding="15dp"
            android:text="青少年球队"
            android:textColor="@color/color_63"
            android:textSize="14sp">
        </TextView>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.2dp"
            android:background="#e4e4e4"
            />

    </LinearLayout>

    <RelativeLayout
        android:id="@+id/add_rel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:background="@color/white"
        android:visibility="gone"
        >

        <TextView
            android:id="@+id/add_cancle_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:padding="15dp"
            android:text="取消"
            android:textColor="@color/color_63"
            android:textSize="14sp">
        </TextView>

        <TextView
            android:id="@+id/add_enter_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:gravity="center"
            android:padding="15dp"
            android:text="确定"
            android:textColor="@color/color_63"
            android:textSize="14sp">
        </TextView>

        <View
            android:id="@+id/line1"
            android:layout_width="match_parent"
            android:layout_height="0.2dp"
            android:layout_below="@+id/add_cancle_tv"
            android:background="#e4e4e4"
            />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="250dp"
            android:layout_below="@+id/line1"
            android:orientation="horizontal"
            >

            <ScrollView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:scrollbars="none"
                >

                <LinearLayout
                    android:id="@+id/provice_lin"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    >
                </LinearLayout>

            </ScrollView>

            <View
                android:layout_width="0.2dp"
                android:layout_height="match_parent"
                android:background="#e4e4e4"
                />

            <ScrollView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:scrollbars="none"
                >

                <LinearLayout
                    android:id="@+id/city_lin"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    >
                </LinearLayout>

            </ScrollView>

            <View
                android:layout_width="0.2dp"
                android:layout_height="match_parent"
                android:background="#e4e4e4"
                />

            <ScrollView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:scrollbars="none"
                >

                <LinearLayout
                    android:id="@+id/area_lin"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    ></LinearLayout>
            </ScrollView>

        </LinearLayout>


    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/touxiang_rel"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone"
        >

        <View
            android:id="@+id/zzc_view"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#70000000"
            />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_above="@+id/bottom_tv"
            android:layout_margin="10dp"
            android:background="@drawable/y_login_edit_bg"
            android:orientation="vertical">

            <TextView
                android:id="@+id/paizhao"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:padding="10dp"
                android:text="拍照"
                android:textColor="#0075fb"
                android:textSize="16sp"/>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.2dp"
                android:background="#e1e1e1"/>

            <TextView
                android:id="@+id/xiangce"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:padding="10dp"
                android:text="从手机相册选择"
                android:textColor="#0075fb"
                android:textSize="16sp"/>
        </LinearLayout>

        <TextView
            android:id="@+id/bottom_tv"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"

            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:background="@drawable/y_login_edit_bg"
            android:gravity="center"
            android:padding="10dp"
            android:text="取消"
            android:textColor="#0075fb"/>
    </RelativeLayout>
</RelativeLayout>