                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="3dp"
                        android:layout_toRightOf="@+id/img_bg"
                        android:text="赛制: --"
                        android:textColor="#9b9b9b"
                        android:textSize="12sp"/>

                </RelativeLayout>

                <RelativeLayout
