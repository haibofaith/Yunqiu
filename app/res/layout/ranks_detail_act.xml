                android:text="取消"
                android:textColor="@color/red"
                android:textSize="14sp"
                />

            <TextView
                android:id="@+id/xiugai_enter"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:background="@drawable/yq_btn_green"
                android:gravity="center"
                android:padding="10dp"
                android:text="确定"
                android:textColor="@color/white"
                android:textSize="14sp"
                />
        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/jiesan_lin"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:layout_marginLeft="50dp"
        android:layout_marginRight="50dp"
        android:background="@color/white"
        android:orientation="vertical"
        android:visibility="gone">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:padding="25dp"
            android:text="确认解散球队？"
            android:textColor="@color/color_63"
            android:textSize="16sp"
            />

        <View
            android:layout_width="match_parent"
            android:layout_height="0.2dp"
            android:background="#e4e4e4"
            />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="horizontal"
            >

            <TextView
                android:id="@+id/jiesan_tv"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:padding="15dp"
                android:text="解散！"
                android:textColor="@color/red"
                android:textSize="16sp"
                />

            <View
                android:layout_width="0.2dp"
                android:layout_height="match_parent"
                android:background="#e4e4e4"
                />

            <TextView
                android:id="@+id/jiesan_cancle"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:padding="15dp"
                android:text="取消"
                android:textColor="@color/color_63"
                android:textSize="16sp"
                />

        </LinearLayout>
    </LinearLayout>


    <LinearLayout
        android:id="@+id/join_lin"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:background="@color/white"
        android:orientation="vertical"
        android:visibility="gone"
        >

        <TextView
            android:id="@+id/join_tv"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:padding="15dp"
            android:text="申请入队"
            android:textColor="@color/color_63"
            android:textSize="14sp"
            />

        <View
            android:layout_width="match_parent"
            android:layout_height="0.2dp"
            android:background="#e4e4e4"
            />

        <TextView
            android:id="@+id/join_yq"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:padding="15dp"
            android:text="邀请码直通车"
            android:textColor="@color/color_63"
            android:textSize="14sp"
            />

        <View
            android:layout_width="match_parent"
            android:layout_height="0.2dp"
            android:background="#e4e4e4"
            />

        <TextView
            android:id="@+id/join_qx"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:padding="15dp"
            android:text="取消"
            android:textColor="@color/color_63"
            android:textSize="14sp"
            />

        <View
            android:layout_width="match_parent"
            android:layout_height="0.2dp"
            android:background="#e4e4e4"
            />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/join_lin_lin"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:layout_marginLeft="50dp"
        android:layout_marginRight="50dp"
        android:background="@color/white"
        android:orientation="vertical"
        android:padding="15dp"
        android:visibility="gone"
        >

        <TextView
            android:id="@+id/join_tvtv"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:padding="15dp"
            android:text="给管理员打个招呼吧"
            android:textColor="@color/color_63"
            android:textSize="14sp"
            />

        <EditText
            android:id="@+id/join_edit"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:background="@drawable/gray_bg_detail"
            android:gravity="center"
            android:hint=""
            android:maxLength="6"
            android:maxLines="1"
            android:padding="15dp"
            android:textColor="@color/color_63"
            android:textSize="14sp"

            />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:gravity="center"
            android:orientation="horizontal"

            >

            <TextView
                android:id="@+id/join_cancle"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:background="@drawable/gray_bg"
                android:gravity="center"
                android:padding="10dp"
                android:text="取消"
                android:textColor="@color/color_63"
                android:textSize="14sp"
                />

            <TextView
                android:id="@+id/join_enter"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:background="@drawable/yq_btn_green"
                android:gravity="center"
                android:padding="10dp"
                android:text="发送"
                android:textColor="@color/white"
                android:textSize="14sp"
                />
        </LinearLayout>

    </LinearLayout>


</RelativeLayout>